#include<bits/stdc++.h>

using namespace std;
typedef unsigned long long ull;

ull arr[100005];
int main(){

    int t;
    cin>>t;
    while(t--){
        int n,k;
        cin>>n>>k;
        for(int i=0;i<n;i++){
            cin>>arr[i];
        }
        int group=n/k;
        sort(arr, arr + n);
        int counter=0;
        int p=n-1;
        int j=0;
        vector<ull> v;
        int i;
        for( i=1;i<n;i++){
            if(arr[i]!=arr[i-1]){
                counter=0;
                while(j<i){
                    v.push_back(arr[j]);
                    j++;
                }
            }else{
                counter++;
                if(counter==k-1){
                    group--;
                    j=i+1;
                    p=p-k;
                    counter=0;
                }
            }
        }
        while(j<i){
            v.push_back(arr[j]);
            j++;
        }
       /* for(int i=0;i<v.size(); i++){
            cout<<v[i]<<" ";
        }
        cout<<endl;*/
       // int i;
        for(i=1;i<v.size();i++){
            if(v[i]!=v[i-1]){
                group--;
            }

            if(group==0){
                break;
            }
        }
        cout<<v.size()-i<<endl;
    }
    return 0;
}
