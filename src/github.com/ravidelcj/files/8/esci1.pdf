#include<iostream>
#include<cstdio>

using namespace std;

int we[109],v[109];
int dp[109][109];
int main(){

    int t;
    scanf("%d",&t);
    while(t--){

        int n;
        scanf("%d",&n);
        int w;
        scanf("%d",&w);
        for(int i=1;i<=n;i++){
            scanf("%d",&v[i]);
        }
        for(int i=1;i<=n;i++){
            scanf("%d",&we[i]);
        }
        for(int i=0;i<=w;i++){
            dp[0][i]=0;
        }
        for(int j=0;j<=n;j++){
            dp[j][0]=0;
        }

        for(int i=1;i<=n;i++){

            for(int j=1;j<=w;j++){
                if(j<we[i]){
                    dp[i][j]=dp[i-1][j];
                }else{
                    dp[i][j]=max(dp[i-1][j],v[i]+dp[i-1][j-we[i]]);
                }

            }
        }
        printf("%d\n",dp[n][w]);
    }
    return 0;
}
